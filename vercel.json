{
    "builds": [
            {
                "src": "chat_app/asgi.py",
                "use": "@vercel/python",
                "config": { "maxLambdaSize": "15mb" , "runTime":"3.9"}
            },
            {
                "src":"build_files.sh",
                "use":"@vercel/static-build",
                "config":{"distDir":"static/"}
            }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "chat_app/asgi.py"
        },
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        }
    ]
}
