{
    "builds": [
            {
                "src": "chat_app/asgi.py",
                "use": "@now/python",
                "config": { "maxLambdaSize": "15mb" , "runTime":"3.9"}
            },
            {
                "src":"build_files.sh",
                "use":"@vercel/static-build",
                "config":{"distDir":"dist/"}
            }
        {
            "src": "chat_app/asgi.py",
            "use": "@now/python"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "chat_app/asgi.py"
        }
    ]
}
